<thlogic>
   <attr sel="#headId" th:replace="~{fragments/head :: headFragment}"/>
    <attr sel="#game-title" th:text="#{game.title}"/>
    <attr sel="#mainMessage" th:text="${__${T(ro.catalin.util.AttributeNames).MAIN_MESSAGE}__}"/>
    <attr sel="#resultMessage" th:text="${__${T(ro.catalin.util.AttributeNames).RESULT_MESSAGE}__}"/>
    <attr sel="#game-form" th:action="@{__${T(ro.catalin.util.GameMappings).PLAY}__}"/>
    <attr sel="#enter-guess" th:text="#{game.enter.guess}"/>
    <attr sel="#submit" th:value="#{game.submit}"/>
    <attr sel="#footerId" th:replace="~{fragments/footer :: footer}"/>
</thlogic>